package Prac;

//Kadaneâ€™s Algorithm

public class MaxSumSubArra {
	public static void main(String[] args) {
		int[] arr = { -2, -3, 4, -1, -2, 1, 5, -3 };
		int currMax=Integer.MIN_VALUE;
		int maxSoFar=Integer.MIN_VALUE;
		for(int i=0;i<arr.length;i++) {
			max=arr[i];
			if (max<0) {
				max=0;
			}else {
				max +=arr[i];
			}
		}
	}
}
